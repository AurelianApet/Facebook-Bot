        {
            if (Model.Count == 1)
            {
                <p>{"data" : [{"id": "@Model[i].id", "name" : "@Model[i].name"}]}</p>
                continue;
            }
            if (i == 0)
            {
                <p>{"data" : [{"id": "@Model[i].id", "name" : "@Model[i].name"},</p>
                continue;
            }
            if (i == Model.Count - 1)
            {
                <p>{"id": "@Model[i].id", "name" : "@Model[i].name"}]}</p>
                continue;
            }
            if (i != 0 && i != Model.Count - 1)
